        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ypf.Mms.Node.Test"
        mc:Ignorable="d"
        Title="MainWindow" Height="270" Width="265" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Width="100" Height="40" Content="Start" Click="Button_Click"/>
            <Button Width="100" Margin="0,10,0,0" Height="40" Content="Stop" Click="Button_Click_1"/>
            <Button Width="100" Margin="0,10,0,0" Height="40" Content="Protocolo" Click="Button_Click_2"/>
        </StackPanel>
    </Grid>
</Window>
